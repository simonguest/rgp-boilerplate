{
  "name": "rgp-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate Template for React, GraphQL, and PostgreSQL application",
  "scripts": {
    "webpack:vendor": "node_modules/.bin/webpack --config=webpack.dll.js",
    "webpack:build": "node_modules/.bin/webpack",
    "webpack:watch": "node_modules/.bin/webpack --watch &",
    "prestart": "npm run webpack:build && npm run webpack:watch",
    "start": "PGDATABASE=postgres node_modules/.bin/nodemon --ignore dist/ index.js",
    "dev": "NODE_ENV=development npm start",
    "prod": "NODE_ENV=production npm start",
    "pg:start": "pg_ctl -D ./data -w start",
    "pg:stop": "pg_ctl -D ./data stop"
  },
  "author": "Simon Guest",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "express": "^4.15.2",
    "express-graphql": "^0.6.3",
    "express-session": "^1.15.1",
    "graphql": "^0.9.1",
    "isomorphic-fetch": "^2.2.1",
    "istanbul-middleware": "^0.2.2",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "pg": "^6.1.4",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "istanbul": "^0.4.5"
  }
}
