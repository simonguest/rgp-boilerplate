{
  "name": "rgp-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate Template for React, GraphQL, and PostgreSQL application",
  "main": "server.js",
  "scripts": {
    "predev": "node_modules/.bin/webpack --watch --progress &",
    "dev": "node_modules/.bin/nodemon --ignore dist/ server.js",
    "pg:start": "pg_ctl -D ./data -l logfile start",
    "pg:stop": "pg_ctl -D ./data stop"
  },
  "author": "Simon Guest",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "express": "^4.15.2",
    "express-graphql": "^0.6.3",
    "express-session": "^1.15.1",
    "graphql": "^0.9.1",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "pg": "^6.1.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack": "^2.2.1"
  }
}
